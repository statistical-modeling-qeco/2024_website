 <Row>
  <Cell><Data ss:Type="String"><h:Font h:Size="7">September 1998</h:Font></Data></Cell>
  <Cell><Data ss:Type="String"></Data></Cell>
  <Cell><Data ss:Type="String"><h:Font h:Size="8">SPECIES LOSS IN ANT COMMUNITIES</h:Font></Data></Cell>
